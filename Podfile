
target 'IndoorIOS' do
  
  source 'https://github.com/CocoaPods/Specs.git'
  platform :ios, '8.1'
  pod 'AFNetworking', '~> 3.0'

  pod 'BaiduMapKit' #百度地图SDK

  pod 'AlipaySDK-2.0' #支付宝SDK，支持bitcode
  pod 'AlipaySDK-2.0/Order' #支付宝订单签名RSASigner MD5Signer，暂不支持bitcode

  pod 'SDWebImage', '~>3.6'

end
